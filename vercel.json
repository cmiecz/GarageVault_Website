{
  "cleanUrls": false,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "x-forwarded-proto",
          "value": "http"
        }
      ],
      "destination": "https://garagevault.app/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "header",
          "key": "host",
          "value": "www.garagevault.app"
        }
      ],
      "destination": "https://garagevault.app/:path*",
      "permanent": true
    },
    {
      "source": "/storage-categories.html",
      "destination": "/support/storage-categories.html",
      "permanent": true
    },
    {
      "source": "/households.html",
      "destination": "/support/households.html",
      "permanent": true
    },
    {
      "source": "/store-reminders.html",
      "destination": "/support/store-reminders.html",
      "permanent": true
    },
    {
      "source": "/store-management.html",
      "destination": "/support/store-management.html",
      "permanent": true
    },
    {
      "source": "/maintenance-calendar-export.html",
      "destination": "/support/maintenance-calendar-export.html",
      "permanent": true
    },
    {
      "source": "/cross-household-sharing.html",
      "destination": "/support/cross-household-sharing.html",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/support",
      "destination": "/support/index.html"
    },
    {
      "source": "/support/",
      "destination": "/support/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/support/(.*\\.html)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}

